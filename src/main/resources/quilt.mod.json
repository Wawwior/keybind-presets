{
	"schema_version": 1,
	"quilt_loader": {
		"group": "me.wawwior",
		"id": "keybind_profiles",
		"version": "${version}",
		"metadata": {
			"name": "Keybind Profiles",
			"description": "Keybind Profiles is a mod that allows you to create and switch between keybind profiles.",
			"contributors": {
				"Wawwior": ["Owner", "Author"]
			},
			"contact": {
				"homepage": "https://wawwior.me/keybind-profiles",
				"issues": "https://github.com/Wawwior/keybind-profiles/issues",
				"sources": "https://github.com/Wawwior/keybind-profiles"
			},
			"icon": "assets/keybind_profiles/icon.png",
			"license": "MIT"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"client_init": "me.wawwior.keybind_profiles.KeybindProfiles"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.19.1"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.2"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			}
		]
	},
	"mixin": "keybind_profiles.mixins.json",
	"minecraft": {
		"environment": "client"
	}
}
